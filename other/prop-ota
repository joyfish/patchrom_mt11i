#!/tmp/busybox sh
miversion=`3.5.29`
mitime=`Å›ro, 29 maj 2013, 12:10:28`
miutc=`1369822228`
miota=`20130529-1210`
cp -f /system/build.prop /tmp/build.prop
chmod 755 /tmp/build.prop
cat /tmp/build.prop | sed -e "s/ro\.build\.date=.*/ro\.build\.date=$mitime/g" \
				| sed -e "s/ro\.build\.date\.utc=.*/ro\.build\.date\.utc=$miutc/g" \
				| sed -e "s/ro\.build\.version\.incremental=.*/ro\.build\.version\.incremental=$miversion/g" \
				| sed -e "s/updater\.time=.*/updater\.time=$miota/g" \
				| sed -e "s/updater\.ver=.*/updater\.ver=$miversion/g" > /tmp/build-new.prop
mv -f /tmp/build-new.prop /system/build.prop
chmod 644 /system/build.prop
